<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-12'>
      <h1><%= @model %> search for "<%= @content %>"</h1>
        <% if @model == 'user' %>
          <%= render 'users/index',users: @records %>
        <% elsif @model == 'book' %>
          <%= render 'books/index',books: @records %>
        <% end %>
    </div>
  </div>
</div>
